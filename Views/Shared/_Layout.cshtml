<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/style.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css" integrity="sha512-iecdLmaskl7CVkqkXNQ/ZH/XLlvWZOJyj7Yy7tcenmpD1ypASozpmT/E0iPtmFIB46ZmdtAc9eNBvH0H/ZpiBw==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <link href="node_modules/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
    <link href="~/css/styles.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/MVC.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <div class="header">
            <div class="header_left">
                <div class="logo">
                    <span class="logoez">Ex</span>
                    <span class="logotrade">Trade</span>
                    
                </div>
                <div class="header_left_list">
                    <div class="header_left-item">
                        <span>Lịch sử giao dịch</span>
                        <div class="first">
                            <a>Lịch sử đặt lệnh</a>
                            <a>Khớp lệnh</a>
                        </div>
                     <div class="triangle first"></div>
                    </div>
                    <div class="header_left-item">
                        <span>GD tiền</span>
                        <div>
                            <a>Chuyển tiền</a>
                            <a>Khớp lệnh</a>
                            <a>Chuuyển tiền kí quỹ Ck phái sinh</a>
                        </div>
                     <div class="triangle"></div>

                    </div>
                    <div class="header_left-item">
                        <span>giao dịch</span>
                        <div>
                            <a>Lịch sử đặt lệnh</a>
                            <a>Khớp lệnh</a>
                        </div>
                     <div class="triangle"></div>

                    </div>
                   
                </div>
            </div>

            <div class="header_right">
                <div>
                    <div class="dropdown">
                    <button class="" type="button" data-bs-toggle="dropdown" aria-expanded="false">
                        <svg width="18" height="12" viewBox="0 0 18 12" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <path d="M0 12H18V10H0V12ZM0 7H18V5H0V7ZM0 0V2H18V0H0Z" fill="blue"/>
                        </svg>
                    </button>
                    <ul class="dropdown-menu">
                        <li><a class="dropdown-item" href="#">Demo</a></li>
                    </ul>
                    </div>
                </div>
                <div>
                    <svg width="20" height="22" viewBox="0 0 18 20" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <path d="M3 7.99999C2.99945 6.75335 3.38723 5.53749 4.10945 4.52137C4.83167 3.50524 5.85247 2.73931 7.03 2.32999C6.9844 2.0443 7.00132 1.75212 7.0796 1.47361C7.15788 1.19509 7.29565 0.936874 7.48341 0.716771C7.67117 0.496669 7.90444 0.31992 8.16714 0.19872C8.42983 0.0775204 8.71569 0.0147552 9.005 0.0147552C9.29431 0.0147552 9.58017 0.0775204 9.84286 0.19872C10.1056 0.31992 10.3388 0.496669 10.5266 0.716771C10.7143 0.936874 10.8521 1.19509 10.9304 1.47361C11.0087 1.75212 11.0256 2.0443 10.98 2.32999C12.1557 2.74098 13.1743 3.50764 13.8946 4.52362C14.615 5.5396 15.0013 6.75456 15 7.99999V14L18 16V17H0V16L3 14V7.99999ZM11 18C11 18.5304 10.7893 19.0391 10.4142 19.4142C10.0391 19.7893 9.53043 20 9 20C8.46957 20 7.96086 19.7893 7.58579 19.4142C7.21071 19.0391 7 18.5304 7 18H11Z" fill="blue"/>
                        </svg>

                </div>
                <div>
                    <span>059C888859</span>
                    <svg width="8" height="6" viewBox="0 0 11 8" fill="none" xmlns="http://www.w3.org/2000/svg">
                    <path d="M4.43172 7.47826L0.466282 2.95652C-0.0166881 2.4058 -0.124466 1.77565 0.142946 1.06609C0.410359 0.356522 0.88672 0.00115942 1.57203 0H9.42664C10.113 0 10.5898 0.355362 10.8572 1.06609C11.1247 1.77681 11.0164 2.40696 10.5324 2.95652L6.56695 7.47826C6.41443 7.65217 6.24921 7.78261 6.07127 7.86956C5.89334 7.95652 5.70269 8 5.49933 8C5.29598 8 5.10533 7.95652 4.9274 7.86956C4.74946 7.78261 4.58423 7.65217 4.43172 7.47826Z" fill="blue"/>
                    </svg>


                </div>
            </div>
           
        </div>
    </header>
    <div class="container">
            @RenderBody()
    </div>

    <script src="node_modules/bootstrap/dist/js/bootstrap.js"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
